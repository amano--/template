{
  "extends": "./configs/tsconfig/nextjs.json",
  "include": [
    "."
  ],
  "exclude": [
    "dist",
    "build",
    "node_modules"
  ],
  "compilerOptions": {
    "allowJs": true,
    "noEmit": true,
    "incremental": true,
    "resolveJsonModule": true,
    "jsx": "preserve",
    "noErrorTruncation": true

  }
}
