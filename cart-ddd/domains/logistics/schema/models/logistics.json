{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "definitions": {
        "DeliveryOrder": {
            "type": "object",
            "properties": {
                "userId": {
                    "type": "string",
                    "title": "userId"
                },
                "purchaseId": {
                    "type": "string",
                    "title": "purchaseId"
                },
                "providerId": {
                    "enum": [
                        "doraemon",
                        "gundam"
                    ],
                    "type": "string",
                    "title": "providerId"
                },
                "deliveryMethodId": {
                    "enum": [
                        "dora",
                        "gufu",
                        "gundam",
                        "suneo"
                    ],
                    "type": "string",
                    "title": "deliveryMethodId"
                },
                "orderAt": {
                    "type": "string",
                    "format": "date-time",
                    "title": "orderAt"
                }
            },
            "required": [
                "deliveryMethodId",
                "orderAt",
                "providerId",
                "purchaseId",
                "userId"
            ]
        }
    }
}
